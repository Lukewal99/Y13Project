<UserControl x:Class="Prototype_UI.NavMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prototype_UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="160"/>
            <ColumnDefinition Width="160"/>
            <ColumnDefinition Width="160"/>
            <ColumnDefinition Width="160"/>
            <ColumnDefinition Width="160"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="160"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="80"/>
            <RowDefinition Height="160"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Navigation Menu" Grid.Column="2" Grid.Row="1"/>
        <Button x:Name="ModelOne" Content="User Control 1" Grid.Column="1" Grid.Row="2" Click="ModelOne_Click"/>
        <Button x:Name="ModelTwo" Content="User Control 2" Grid.Column="2" Grid.Row="2" Click="ModelTwo_Click"/>
        <Button x:Name="ModelThree" Content="User Control 3" Grid.Column="3" Grid.Row="2" Click="ModelThree_Click"/>
    </Grid>
</UserControl>
